                        android:textColor="#0B0A0A"
                        android:textSize="20sp"
                        android:textStyle="normal|bold" />

                </LinearLayout>

                <Space
                    android:layout_width="match_parent"
                    android:layout_height="17dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="94dp"
                    android:orientation="horizontal">

                    <Space
