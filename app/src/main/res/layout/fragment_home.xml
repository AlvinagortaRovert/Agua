                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView233"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:layout_gravity="center|center_horizontal"
                        android:text="Accesos rÃ¡pido"
